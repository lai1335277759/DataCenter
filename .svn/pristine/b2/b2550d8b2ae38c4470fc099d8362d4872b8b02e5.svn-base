.app-main{
    .el-form{
        margin: 22px 0;
        padding: 25px 20px 5px 20px;
        background-color: #ffffff;
        box-shadow: 0px 2px 6px 0px 
            rgba(109, 139, 255, 0.04);
        border-radius: 5px;
    }
}
.table-content{
    padding: 26px;
	height: 600px;
	background-color: #ffffff;
	box-shadow: 0px 2px 6px 0px 
		rgba(109, 139, 255, 0.04);
    border-radius: 5px;
    h3{
        display: inline-block;
        padding-bottom: 10px;
        margin-bottom: 20px;
        font-size: 20px;
        font-weight: normal;
        font-stretch: normal;
        letter-spacing: 1px;
        color: #555555;
        border-bottom:4px solid #ffc500;
    }
    .page{
        margin-top: 20px;
    }
}